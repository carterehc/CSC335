/**
 * Sets any 16 bit input to zero.
   To be used in ALU16, or any other hdl that requires presetting of 16 bit input
 */

CHIP Zero16{
    IN in[16];
    OUT out[16];

    PARTS:
    /*Assignment: Project 2, CSC 335, Berea College
      Name: Emily Carter - cartere
      Date: 09/15/17
      Instructor: Mario Nakazawa*/
	  
	Not(in=in[0], out=notin0);
	And(a=in[0], b=notin0, out=out[0]);
	
	Not(in=in[1], out=notin1);
	And(a=in[1], b=notin1, out=out[1]);
	
	Not(in=in[2], out=notin2);
	And(a=in[2], b=notin2, out=out[2]);
	
	Not(in=in[3], out=notin3);
	And(a=in[3], b=notin3, out=out[3]);
	
	Not(in=in[4], out=notin4);
